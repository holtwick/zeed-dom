{
  "name": "zeed-dom",
  "type": "module",
  "version": "0.10.3",
  "description": "ğŸŒ± Lightweight offline DOM",
  "author": {
    "name": "Dirk Holtwick",
    "email": "dirk.holtwick@gmail.com",
    "url": "https://holtwick.de"
  },
  "license": "MIT",
  "funding": {
    "type": "GitHub Sponsors â¤",
    "url": "https://github.com/sponsors/holtwick"
  },
  "homepage": "https://github.com/holtwick/zeed-dom",
  "repository": {
    "type": "git",
    "url": "https://github.com/holtwick/zeed-dom.git"
  },
  "bugs": {
    "url": "https://github.com/holtwick/zeed-dom/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "require": "./dist/index.cjs",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "browser": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "browser": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs",
      "import": "./dist/jsx-runtime.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "jsx-runtime"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup src/index.ts src/jsx-runtime.ts --dts --sourcemap --format esm,cjs",
    "check": "tsc --noEmit -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm test && npm run build",
    "start": "npm run watch",
    "test": "vitest src --coverage --globals --run",
    "watch": "npm run build:tsup -- --watch"
  },
  "dependencies": {
    "css-what": "^6.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@antfu/ni": "^0.20.0",
    "@types/node": "^18.15.3",
    "@vitest/coverage-c8": "^0.29.3",
    "c8": "^7.13.0",
    "eslint": "^8.36.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  }
}
